import java.util.* ;
public class Main{

     public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int num =0;
        int j=0;
        while (n>0)
        {
            int temp = n%10;
            for (int i=0;i<j;i++)
            {
                temp =  temp*2;
            }
            num = num + temp;
            j+=1;
            n=n/10;
        }
        System.out.println(num);
     }
}
